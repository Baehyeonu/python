   ],
   "source": [
    "def add():\n",
    "    numbers = input(\"두개의 숫자를 입력해 주세요.\").split()\n",
    "    sum = 0\n",
    "    for num in numbers:\n",
    "        sum += int(num)\n",
    "    print(f'입력한 숫자들의 합: {sum}')\n",
    "\n",
    "add()\n",
    "print(type(add))\n",
    "print(type(sum))"
   ]
  }
